    "#number guessing game\n",
    "import random\n",
    "random_num=random.randint(1,100)\n",
    "while True:\n",
    "    user_guess=int(input(\"enter a number\"))\n",
    "    if user_guess== random_num:\n",
    "        print(\"Correct\")\n",
    "        break\n",
    "    elif user_guess<random_num:\n",
    "        print(\"Higher\")\n",
    "    else:\n",
    "        print(\"Lower\")"
   ]
